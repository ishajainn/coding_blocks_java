import java.util.*;
public class Main {
    public static void main (String args[]) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int arr[]=new int[n];
        for(int i=0;i<n;i++){
            int num=sc.nextInt();
            arr[i]=num;
        }
        int c=-1;
        int num1=sc.nextInt();
        for(int j=0;j<n;j++){
            if(arr[j]==num1)
            c=j;
           
        }
         System.out.println(c);

    }
}
